{
  "name": "vuewebpack4",
  "version": "1.0.1",
  "main": "index.js",
  "description": "A Vuejs template using Webpack 4",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/* && webpack --config webpack.config.prod.js",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development PORT=3000 node server.js",
    "start": "cross-env NODE_ENV=production PORT=80 node server.js",
    "prod": "npm run build && cross-env NODE_ENV=production PORT=3000 node server.js"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "author": "Sampson Li",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@vue/babel-preset-app": "^3.3.0",
    "autoprefixer": "^9.1.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "forward-request": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "es6-promise": "^4.2.5",
    "scroll-accelerate": "^1.0.1",
    "v-tap": "^3.0.3",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "ios >= 8",
    "android >= 4.0"
  ]
}
