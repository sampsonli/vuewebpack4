{
  "name": "vuewebpack4",
  "version": "2.0.1",
  "main": "index.js",
  "description": "A Vuejs template using Webpack 4",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/* && cross-env PUB_PATH='' webpack --config build/webpack.config.prod.js",
    "build:analyse": "cross-env ANALYSE=1 npm run build",
    "build:dll": "webpack --config build/webpack.config.dll.js",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development PUB_PATH='' PORT=3000 node server.js",
    "start": "cross-env NODE_ENV=production PORT=80 node server.js",
    "prod": "npm run build && cross-env NODE_ENV=production PUB_PATH='' PORT=3000 node server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "author": "Sampson Li",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "core-js": "^3.6.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.1",
    "express": "^4.16.4",
    "file-loader": "^5.0.2",
    "forward-request": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "lint-staged": "^9.5.0",
    "husky": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "es6-promise": "^4.2.5",
    "scroll-accelerate": "^1.0.1",
    "v-tap": "^3.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "vuex-router-sync": "^5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "ios >= 8",
    "android >= 4.0"
  ]
}
